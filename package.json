{
  "name": "k-spots-app",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:en": "cross-env VITE_I18N_LANG=en vite",
    "dev:zh": "cross-env VITE_I18N_LANG=zh vite",
    "dev:ja": "cross-env VITE_I18N_LANG=ja vite",
    "build": "vite build",
    "build:en": "cross-env VITE_I18N_LANG=en vite build --outDir dist/en",
    "build:zh": "cross-env VITE_I18N_LANG=zh vite build --outDir dist/zh",
    "build:ja": "cross-env VITE_I18N_LANG=ja vite build --outDir dist/ja",
    "build:all": "npm run build -- --outDir dist/ko && npm run build:en && npm run build:zh && npm run build:ja",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.8.0",
    "axios": "^1.6.2",
    "exifr": "^7.1.3",
    "lucide-react": "^0.294.0",
    "mp4box": "^0.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-player": "^2.14.1",
    "react-router-dom": "^6.20.0",
    "swiper": "^11.0.5",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/node": "^25.0.2",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "cross-env": "^10.1.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  }
}
